{
  "expo": {
    "name": "herfer-app",
    "slug": "herfer-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "androidMode": "default",
      "androidCollapsedTitle": "Alertas de Stock",
      "iosDisplayInForeground": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.axeljhosmell123.herferapp",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "permissions": [
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM",
        "WAKE_LOCK",
        "POST_NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.axeljhosmell123.herferapp"
    },
    "web": {},
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "androidMode": "default",
          "androidCollapsedTitle": "Alertas de Stock",
          "iosDisplayInForeground": true
        }
      ],
      [
        "expo-background-fetch",
        {
          "backgroundFetchInterval": 10
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9194895c-a18c-42ce-90fe-2cd6d1d20945"
      }
    }
  }
}