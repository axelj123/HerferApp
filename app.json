{
  "expo": {
    "name": "herfer-app",
    "slug": "herfer-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "notification": {
      "androidMode": "default",
      "androidCollapsedTitle": "Alertas de Stock",
      "iosDisplayInForeground": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.axeljhosmell123.herferapp",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.axeljhosmell123.herferapp"
    },
    "web": {},
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "30d8f052-b94b-4003-a6d3-a1aa059b9c04"
      }
    },
    "owner": "lunalunera"
  }
}
